public class tax{
  public static void main(String[] args){
    int income=200000;
    float tax;
    if(income<=150000){
      System.out.println("No tax");
    }else if(income>150000 && income<=300000){
      tax=income/10;
      System.out.println(tax);
    }else if(income>300000 && income<=500000){
      tax=income/20;
      System.out.println(tax);
    }else{
      tax=income/30;
      System.out.println(tax);
    }
  }
}
