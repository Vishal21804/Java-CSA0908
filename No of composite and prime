public class comppri{
  public static void main(String[] args){
    int arr[]={4,54,29,71,7,59,98,23};
    int comp=0,prime=0;
    for(int i=0;i<arr.length;i++){
      int c=0;
      for(int j=2;j<arr[i];j++){
        if(arr[i]%j==0){
          c++;
        }
      }if(c>0){
        comp++;
      }else{
        prime++;
      }
    }
    System.out.println(comp);
    System.out.println(prime);
  }
}
