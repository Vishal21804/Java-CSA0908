public class palindrome{
  public static void main(String[] args){
    String s1="MADAM";
    String s2="";
    int length=s1.length();
    for(int i=length-1;i>=0;i--){
      s2=s2+s1.charAt(i);
    }
    if(s1.equals(s2)){
      System.out.println("Palindrome");
    }else{
      System.out.println("Not Palindrome");
    }
  }
}
