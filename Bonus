public class bonus{
  public static void main(String[] args){
    String grade= "a";
    double salary=9000;
    double bonus=0;
    if(grade=="A"||grade=="a"){
      bonus=salary*0.05;
    }else if(grade=="B"||grade=="b"){
      bonus=salary*0.10;
    }
    if(salary<10000){
      bonus+=salary*0.02;
      }
      double total=salary+bonus;
      System.out.println(bonus);
      System.out.println(total);
  }
}
