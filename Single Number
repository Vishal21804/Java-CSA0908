public class R192210513 {

    public int singleNumber(int[] nums) {
        int unique = 0;
        for (int num : nums) {
            unique ^= num;
        }
        return unique;
    }

    public static void main(String[] args) {
        R192210513 finder = new R192210513();
        int[] nums = {2, 2, 1};
        System.out.println("The single number is: " + finder.singleNumber(nums)); 
    }
}
